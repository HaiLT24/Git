        <i class="fas fa-angle-up"></i>
    </a>

    <!-- Bootstrap core JavaScript-->
    <script src="/Areas/Admin/Asset/vendor/jquery/jquery.min.js"></script>
    <script src="/Areas/Admin/Asset/vendor/bootstrap/js/bootstrap.bundle.min.js"></script>

    <!-- Core plugin JavaScript-->
    <script src="/Areas/Admin/Asset/vendor/jquery-easing/jquery.easing.min.js"></script>

