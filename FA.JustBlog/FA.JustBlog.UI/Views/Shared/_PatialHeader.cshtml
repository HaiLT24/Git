<!-- Page Header -->
<header class="masthead" style="background-image: url('@ViewBag.UrlImage')">
    <div class="overlay"></div>
    <div class="container">
        <div class="row">
            <div class="col-lg-8 col-md-10 mx-auto">
                <div class="site-heading">
                    <h1>@ViewBag.Title</h1>
                    <span class="subheading">@ViewBag.Note</span>
                </div>
            </div>
        </div>
    </div>
</header>
